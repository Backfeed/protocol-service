{
  "custom": {
    "excludePatterns": [],
    "envVars": [],
    "optimize": true
  },
  "handler": "biddings/getbiddingcontribut/handler.handler",
  "name": "getbiddingcontribut",
  "runtime": "nodejs",
  "timeout": 6,
  "memorySize": 1024,
  "eventSourceArn": "",
  "endpoints": [
    {
      "path": "biddings/{id}/contributions",
      "method": "GET",
      "authorizationType": "none",
      "apiKeyRequired": true,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": "{\"id\":\"$input.params('id')\" , \"userId\":\"$input.params('userId')\"}"
      },
      "responses": {
        "400": {
          "statusCode": "400",
          "selectionPattern": "(?!404|409)(4[0-9][0-9]).*",
          "responseParameters": {},
          "responseTemplates": {
            "application/json": "#set($inputRoot = $input.path('$'))\n{\"error\": \"$inputRoot.errorMessage.substring(4)\"}"
          }
        },
        "404": {
          "statusCode": "404",
          "selectionPattern": "404.*",
          "responseParameters": {},
          "responseTemplates": {
            "application/json": "#set($inputRoot = $input.path('$'))\n{\"error\": \"$inputRoot.errorMessage.substring(4)\"}"
          }
        },
        "409": {
          "statusCode": "409",
          "selectionPattern": "409.*",
          "responseParameters": {},
          "responseTemplates": {
            "application/json": "#set($inputRoot = $input.path('$'))\n{\"error\": \"$inputRoot.errorMessage.substring(4)\"}"
          }
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": "#set($inputRoot = $input.path('$'))\n$inputRoot"
          }
        }
      }
    }
  ]
}
